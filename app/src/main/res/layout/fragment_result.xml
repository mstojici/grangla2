<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.boardgame.miljac.grangla.gameUI.ResultFragment">


    <ProgressBar
        android:id="@+id/result_bar"
        style="@android:style/Widget.ProgressBar.Horizontal"
        android:layout_centerInParent="true"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:max="100"
        android:progress="33"
        android:progressDrawable="@drawable/progress_drawable_result" />

    <ProgressBar
        android:id="@+id/result_bar2"
        style="@android:style/Widget.ProgressBar.Horizontal"
        android:layout_centerInParent="true"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:max="100"
        android:progress="67"
        android:progressDrawable="@drawable/progress_drawable_result2" />

</FrameLayout>
